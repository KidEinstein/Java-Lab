import java.applet.Applet;
import java.awt.Button;
import java.awt.TextField;


public class FactorialReverseAwt extends Applet {
	
	public void init() {
		TextField numberTextField = new TextField(10);
		add(numberTextField);
		Button fact = new Button("Fact");
		Button reverse = new Button("Reverse");
		add(fact);
		add(reverse);
	}
	
	
}
